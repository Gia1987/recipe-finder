        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.13.0",
        "jest": "^24.8.0",
        "jest-dom": "^3.2.2",
        "prettier": "^1.17.1",
        "react-test-renderer": "^15.6.2",
        "react-testing-library": "^7.0.0",
        "url-loader": "^1.1.2"
    },
    "dependencies": {
        "@babel/polyfill": "^7.4.4",
        "axios": "^0.21.1",
        "axios-mock-adapter": "^1.17.0",
