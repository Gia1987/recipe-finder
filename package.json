        "path": "^0.12.7",
        "prop-types": "^15.7.2",
        "react": "^16.10.1",
        "react-dom": "^16.10.1",
        "react-i18next": "^10.13.1",
        "react-redux": "^7.0.3",
        "react-router-dom": "^5.0.0",
        "react-spinners": "^0.5.13",
        "redux": "^4.0.1",
        "redux-logger": "^3.0.6",
        "redux-mock-store": "^1.5.3",
        "redux-thunk": "^2.3.0",
